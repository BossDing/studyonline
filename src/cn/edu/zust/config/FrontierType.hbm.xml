<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.zust.entity.FrontierType" table="S_FRONTIER_TYPE">
		<id name="id">
			<generator class="sequence">
				<param name="sequence">s_frontiertype_seq</param>
			</generator>
		</id>
		<property name="frontierType" type="string">
			<column name="FRONTIER_TYPE" length="100" />
		</property>
	</class>
</hibernate-mapping>
